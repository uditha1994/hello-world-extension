{
    "manifest_version": 3,
    "name": "Back Color Changer",
    "version": "1.0.0",
    "description": "A simple tool to change webpages background color ",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "BackColor Changer"
    },
    "icons": {
        "24": "icons/icon24.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    }
}